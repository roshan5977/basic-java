package com.jsp.codewithlucky;

class A
{
	int i;  //non static data member
	//default constructor
public A()
{
	i=10;
}
//constructor is a special type of method which has the same name as the class name;
//constructor is nothing  but a set of statement that is executed at the time of object creation
//constructor initialises the object by default at the time of instantiation;
//constructor does not have a return type even it does not have void too;
//constructor cannot be static as it always deals with objects;
//for every object created constructor is called by default so its called constructor call;

}
public class BasicConstructor1 {

	public static void main(String[] args) {
		
         A a1 = new A();
         A a2 = new A();
         System.out.println("a1.i="+a1.i);
         System.out.println("a2.i="+a2.i);
         
         
	}

}
