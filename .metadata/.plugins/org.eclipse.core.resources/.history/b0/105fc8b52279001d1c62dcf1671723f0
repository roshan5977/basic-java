package com.jsp.twoDArray;

public class SumOfEachRow {

	public static void main(String[] args) {
     int [][]a= {
    		 {1,2,3},
    		 {4,5,6},
    		 {7,8,9}
     };
     int sum=0;
     for(int[]x:a)
     {
    	 for(int b:x)
    	 {
    		 sum+=b;
    	 }
    	 System.out.println(sum);
     }
     
	}

}
