package com.jsp.twoDArray;

public class SmallestNum {

	public static void main(String[] args) {
      int[][]a= {
    		  {6,8,2},
    		  {8,3,6},
    		  {5,1,8}
      };
      int res=a[0][0];
      for(int []x:a)
      {
    	  for(int v:x)
    	  {
    		  if(v<res)res=v;
    	  }
      }
      System.out.println("THE SMALLEST NUM IS : " +res);
	}

}
