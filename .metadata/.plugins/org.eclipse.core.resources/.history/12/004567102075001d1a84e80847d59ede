package com.jsp.stringprogram;
import java.util.*;
public class Panagram {
	public static String main(String[] args) {
		String s=new Scanner(System.in).next();
        for(char x='a';x<='z';x++)
        {
                if(s.indexOf(x)==-1) {
                	System.out.println("not an anagram");
                	return "anagram";
                }
        }
       // System.out.println("anagram");
		return "anagram";
	}

}
