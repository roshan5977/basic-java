package com.jsp.linkedlist;

public class LinkedList {

	private Node first = null;
	int count=0;
    Node curr=null;
    Node last=first;

	public void add(Object  ele) {
		if (first==null) {
        first =new Node(ele,null);
        count++;
        return;
		}
		while(first.next!=null)first=first.next;
			first=new Node(ele,null);
			count++;
	}
	public int size()
	{
		return count;
	}
	public void remove()
	{
		
	}
	public void add(int Index,Object ele)
	{
		
	}
	public void clear()
	{
		first=null;
		count=0;
	}
}
