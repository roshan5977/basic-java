package com.jsp.map;

import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class StngPrgm {

	public static void main(String[] args) {
        String s="banana";
        while(s.length()>0)
        {
        char c=s.charAt(0);
        	Pattern p=Pattern.compile("[c]");
        	Matcher m=p.matcher(s);
        	
        }
	}

}
